{
  "name": "gridu-node-dawid-boisse",
  "version": "1.1.0",
  "scripts": {
    "build": "npm install && tsc --build",
    "test": "jest --coverage --bail --silent",
    "start": "node ./build/Application.js",
    "restart": "npm install && tsc --build && DEBUG_LOG=true node ./build/Application.js",
    "docker": "docker-compose down && docker-compose up --build"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "knex": "^1.0.3",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/mysql": "^2.15.21",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*",
      "src/{!(tests),}.ts"
    ]
  },
  "license": "MIT"
}
